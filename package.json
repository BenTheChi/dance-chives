{
  "name": "dance-chives",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "db:migrate:staging": "dotenv -e .env.staging -- npx prisma migrate deploy",
    "db:push:staging": "dotenv -e .env.staging -- npx prisma db push",
    "db:migrate:production": "dotenv -e .env.production -- npx prisma migrate deploy",
    "db:push:production": "dotenv -e .env.production -- npx prisma db push",
    "build": "dotenv -e .env.production -- prisma generate && next build",
    "build:staging": "npm run db:generate:staging && APP_ENV=staging dotenv -e .env.staging -- next build",
    "start": "next start",
    "start:staging": "APP_ENV=staging dotenv -e .env.staging -- next start",
    "staging": "npm run build:staging && npm run start:staging",
    "lint": "eslint .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "db:generate": "dotenv -e .env.development -- prisma generate",
    "db:generate:staging": "dotenv -e .env.staging -- prisma generate",
    "db:generate:production": "dotenv -e .env.production -- prisma generate",
    "db:studio": "dotenv -e .env.development -- prisma studio",
    "db:studio:staging": "dotenv -e .env.staging -- prisma studio",
    "db:studio:production": "dotenv -e .env.production -- prisma studio",
    "db:push": "dotenv -e .env.development -- prisma db push --accept-data-loss",
    "db:reset": "dotenv -e .env.development -- prisma migrate reset --force",
    "db:seed": "dotenv -e .env.development -- prisma db seed && npm run neo4j:seed",
    "neo4j:seed": "dotenv -e .env.development -- tsx scripts/seed-neo4j.ts",
    "neo4j:seed:staging": "dotenv -e .env.staging -- tsx scripts/seed-neo4j.ts",
    "neo4j:seed:production": "dotenv -e .env.production -- tsx scripts/seed-neo4j.ts",
    "export:seed": "dotenv -e .env.development -- tsx scripts/export-seed-data.ts",
    "export:seed:staging": "dotenv -e .env.staging -- tsx scripts/export-seed-data.ts",
    "dev:setup": "npm run docker:up && sleep 10 && npm run dev",
    "db:setup": "docker-compose up prisma-setup",
    "postinstall": "dotenv -e .env.development -- prisma generate || (echo 'Warning: .env.development not found or prisma generate failed. This is normal on first install. Run \"npm run db:generate\" after setting up your .env.development file.' && exit 0)",
    "nuke": "dotenv -e .env.staging -- tsx scripts/nuke-staging.ts"
  },
  "dependencies": {
    "@auth/neo4j-adapter": "^2.8.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.933.0",
    "@google-cloud/storage": "^7.16.0",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.1.8",
    "chrono-node": "^2.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.482.0",
    "neo4j-driver": "^5.28.1",
    "next": "16.0.7",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.2.1",
    "react-big-calendar": "^1.19.4",
    "react-day-picker": "^9.11.2",
    "react-dom": "19.2.1",
    "react-easy-crop": "^5.5.6",
    "react-hook-form": "^7.55.0",
    "resend": "^6.5.2",
    "server-only": "^0.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.7",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "19.2.3",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jest": "^29.7.0",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "uuid": "^11.0.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
