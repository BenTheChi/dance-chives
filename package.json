{
  "name": "dance-chives",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "db:migrate:staging": "NODE_ENV=staging npx prisma migrate deploy",
    "db:push:staging": "NODE_ENV=staging npx prisma db push",
    "build": "next build",
    "build:staging": "NODE_ENV=staging next build",
    "start": "next start",
    "start:staging": "NODE_ENV=staging next start",
    "staging": "npm run build:staging && npm run start:staging",
    "lint": "eslint .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push --accept-data-loss",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed && npm run neo4j:seed",
    "neo4j:seed": "dotenv -e .env.local -e .env -- tsx scripts/seed-neo4j.ts",
    "export:seed": "dotenv -e .env.local -e .env -- tsx scripts/export-seed-data.ts",
    "dev:setup": "npm run docker:up && sleep 10 && npm run dev",
    "db:setup": "docker-compose up prisma-setup"
  },
  "dependencies": {
    "@auth/neo4j-adapter": "^2.8.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.933.0",
    "@google-cloud/storage": "^7.16.0",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.482.0",
    "neo4j-driver": "^5.28.1",
    "next": "16.0.7",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "react": "19.2.1",
    "react-big-calendar": "^1.19.4",
    "react-day-picker": "^9.11.3",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.55.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "19.2.7",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "19.2.3",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jest": "^29.7.0",
    "prisma": "^6.10.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "uuid": "^11.0.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "next-auth": {
      "next": "$next"
    }
  }
}
