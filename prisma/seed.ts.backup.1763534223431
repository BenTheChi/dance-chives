  for (const session of sessions) {
    try {
      await insertSession(session);
      console.log(`‚úÖ Created session: ${session.sessionDetails.title}`);
    } catch (error) {
      console.error(`‚ö†Ô∏è  Error creating session ${session.id}:`, error);
    }
  }

  // Set video roles
  console.log(`üå± Setting ${4} video role relationships...`);
  const videoRoles = [
      {
        userId: "test-user-2",
        videoId: "video-1-breaking-1v1-moderator-r1",
        role: "DANCER",
        eventId: "event-3-new-york-moderator",
        sectionId: "c17c8553-7df5-4509-ba26-cb918b2f499e",
        bracketId: "bracket-1-breaking-1v1-moderator"
      },
      {
        userId: "test-user-3",
        videoId: "video-1-breaking-1v1-admin-r1",
        role: "DANCER",
        eventId: "event-4-seattle-admin",
        sectionId: "04e8a604-eae6-4c40-839d-425b3d21bc95",
        bracketId: "bracket-1-breaking-1v1-admin"
      },
      {
        userId: "test-user-3",
        videoId: "video-1-breaking-1v1-admin-r1",
        role: "WINNER",
        eventId: "event-4-seattle-admin",
        sectionId: "04e8a604-eae6-4c40-839d-425b3d21bc95",
        bracketId: "bracket-1-breaking-1v1-admin"
      },
      {
        userId: "test-user-2",
        videoId: "video-1-waacking-2v2-seattle-r1",
        role: "DANCER",
        eventId: "event-2-seattle-creator",
        sectionId: "3228fe3b-ecfc-4dc0-8678-21582bd27ed6",
        bracketId: "bracket-1-waacking-2v2-seattle"
      }
    ];

  for (const videoRole of videoRoles) {
    try {
      const roles = [videoRole.role];
      if (videoRole.role === "WINNER") {
        roles.push("DANCER"); // Ensure DANCER is present if WINNER is specified
      }
      await setVideoRoles(videoRole.eventId, videoRole.videoId, videoRole.userId, roles);
    } catch (error) {
      console.error(`‚ö†Ô∏è  Error setting video role for ${videoRole.userId} in ${videoRole.videoId}:`, error);
    }
  }

  // Set event roles
  console.log(`üå± Setting ${1} event role relationships...`);
  const eventRoles = [
      {
        eventId: "event-4-seattle-admin",
        userId: "test-user-3",
        role: "PHOTOGRAPHER"
      }
    ];

  for (const eventRole of eventRoles) {
    try {
      await setEventRoles(eventRole.eventId, eventRole.userId, eventRole.role);
    } catch (error) {
      console.error(`‚ö†Ô∏è  Error setting event role for ${eventRole.userId} in ${eventRole.eventId}:`, error);
    }
  }

  // Add team members
  console.log(`üå± Adding ${1} team member relationships...`);
  const teamMembers = [
      {
        userId: "test-user-4",
        targetId: "session-2-popping-seattle",
        targetType: "Session"
      }
    ];

  for (const teamMember of teamMembers) {
    try {
      if (teamMember.targetType === "Event") {
        await addTeamMember(teamMember.targetId, teamMember.userId);
      } else if (teamMember.targetType === "Workshop") {
        await addWorkshopTeamMember(teamMember.targetId, teamMember.userId);
      } else if (teamMember.targetType === "Session") {
        await addSessionTeamMember(teamMember.targetId, teamMember.userId);
      }
    } catch (error) {
      console.error(`‚ö†Ô∏è  Error adding team member ${teamMember.userId} to ${teamMember.targetType} ${teamMember.targetId}:`, error);
    }
  }

  console.log("üéâ PostgreSQL seeding completed!");
}

main()
  .catch((e) => {
    console.error("‚ùå PostgreSQL seeding failed:", e);
    process.exit(1);
  })
  .finally(async () => {
    await prisma.$disconnect();
    await driver.close();
    console.log("‚úÖ Database connections closed");
  });
